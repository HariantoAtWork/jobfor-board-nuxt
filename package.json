{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node .output/server/index.mjs",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "build-preview": "npm run build && npm run start",
    "postinstall": "nuxt prepare",
    "deploy": "rm -rf .output && pnpm run build && npx wrangler pages deploy dist",
    "auth:generate": "npx @better-auth/cli generate -y --config ./modules/0000-auth/lib/auth.server.js",
    "auth:migrate": "npx @better-auth/cli migrate -y --config ./modules/0000-auth/lib/auth.server.js",
    "db:migrate": "npx knex migrate:latest --knexfile knexfile.js",
    "db:migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.js",
    "db:migrate:status": "npx knex migrate:status --knexfile knexfile.js",
    "db:seed": "npx knex seed:run --knexfile knexfile.js",
    "db:make-migration": "npx knex migrate:make --knexfile knexfile.js",
    "db:make-seed": "npx knex seed:make --knexfile knexfile.js",
    "migrate": "./scripts/migrate.sh",
    "app:reset": "rm -rf .output && rm -rf data && rm -rf node_modules && bun install",
    "ci": "./scripts/install.sh"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/icon": "2.0.0",
    "@vueuse/core": "^13.7.0",
    "better-auth": "^1.3.7",
    "better-sqlite3": "^12.2.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.2",
    "knex": "^3.1.0",
    "marked": "^16.2.0",
    "nodemailer": "^7.0.6",
    "nuxt": "^4.0.3",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748",
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0"
  }
}